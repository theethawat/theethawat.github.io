    <style>
      h1,
      h2,
      h3,
      h4,
      blockquote {
        font-family: "kanit", sans-serif;
      }

      h1 {
        font-size: 32px;
      }

      h2 {
        font-size: 28px;
        font-weight: bold;
        padding-top: 0.4em;
        padding-bottom: 0.4em;
      }

         h3 {
        font-size: 20px;
        font-weight: bold;
        padding-top: 0.4em;
        padding-bottom: 0.4em;
      }

      img {
        display: block;
        margin-right: auto;
        margin-left: auto;
        max-height: 512px;
        width: auto;
      }

      p {
        margin-bottom: 1em;
      }

  ul {
   list-style-type: square;
    list-style-position: inside;
   margin-left:2em;
